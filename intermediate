declare module square;

<<module square>>
takes input [x:integer];
returns [value:integer];
start
end


<<<driver program>>>
start
	declare v_1_2_3:integer;
	get_value(v_1_2_3);
	declare result_:real;
	declare a:array[1..10] of real;
	use module square with parameters v_1_2_3;
	print(result_);

	declare sk:integer;
	switch(sk)
	start
		case TRUE:
			print(sk);
			break;
		case FALSE:
			print(sk);
			break;
		default:
			print(sk);
			break;
	end
end




